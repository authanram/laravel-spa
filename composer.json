{
    "name": "authanram/laravel-spa",
    "type": "package",
    "description": "...",
    "keywords": [
        "authanram",
        "laravel",
        "spa"
    ],
    "license": "MIT",
    "homepage": "https://authanram.com",
    "require": {
        "php": "^8.1.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/phpinsights": "^2.2",
        "orchestra/testbench": "^7.6",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\LaravelSpa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\LaravelSpa\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Authanram\\LaravelSpa\\LaravelSpaServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "coverage": "@php ./vendor/bin/pest --coverage",
        "coverage:setup": "pecl install pcov",
        "fix": "export PHPINSIGHTS_ENV= && php ./vendor/bin/phpinsights --fix",
        "insights": "export PHPINSIGHTS_ENV= && php ./vendor/bin/phpinsights",
        "test": "@php ./vendor/bin/pest",
        "test:insights": "export PHPINSIGHTS_ENV=testing && php ./vendor/bin/phpinsights analyse tests"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
